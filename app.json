{
  "expo": {
    "name": "Apple News UI",
    "slug": "apple-news-ui",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "extra": {
      "eas": {
        "projectId": "52ab6a77-bd0c-4c40-9952-d272a4e0781a",
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "bundleIdentifier": "com.sportapp.anews-ui.widget",
                  "targetName": "widget",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.sportapp.anews-ui"
                    ]
                  }
                }
              ]
            }
          }
        }
      }
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "1sizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "appleTeamId": "F7P5RH5CTP",
      "supportsTablet": true,
      "bundleIdentifier": "com.sportapp.anews-ui",
      "deploymentTarget": "15.1",
      "infoPlist": {
        "NSSupportsLiveActivities": true
      },
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.sportapp.anews-ui",
          "group.com.sportapp.anews-ui"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sportapp.anews-ui"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1"
          }
        }
      ],
      "@bacons/apple-targets"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactServerFunctions": true,
      "reactCompiler": true
    },
    "newArchEnabled": true,
    "owner": "qlur",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/52ab6a77-bd0c-4c40-9952-d272a4e0781a"
    }
  }
}
